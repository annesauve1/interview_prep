    "class Solution:\n",
    "\n",
    "    def count(self, seq: str) -> str:\n",
    "        print('seq=',seq)\n",
    "        sol = [] \n",
    "        for s in seq: \n",
    "            if len(sol) == 0: \n",
    "                sol.append((s,1)) \n",
    "            else: \n",
    "                k,v = sol[-1] \n",
    "                if k == s:\n",
    "                    v += 1\n",
    "                    sol[-1] = (k,v)\n",
    "                else: \n",
    "                    sol.append((s,1))\n",
    "        rs = \"\".join([str(value)+str(element) for (element,value) in sol])                       \n",
    "        return rs\n",
    "\n",
    "\n",
    "    def countAndSay(self, n: int) -> str:\n",
    "\n",
    "        it, res = 1, \"1\"\n",
    "\n",
    "        while it < n:\n",
    "\n",
    "            res = self.count(res)\n",
    "\n",
    "            it += 1\n",
    "\n",
    "        return res\n",
    "\n",
